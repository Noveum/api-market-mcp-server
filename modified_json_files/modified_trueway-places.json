            },
            "example": "37.783366,-122.402325"
          },
          {
            "name": "radius",
            "in": "query",
            "description": "The distance (in meters) within which to return results. Max = 10000 m.",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10000
            },
            "example": 180
          },
          {
            "name": "type",
            "in": "query",
            "description": "The type of places that are returned",
            "schema": {
              "type": "string"
            },
            "example": "cafe"
          },
          {
            "name": "language",
