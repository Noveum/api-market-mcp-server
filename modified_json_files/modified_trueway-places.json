{
  "openapi": "3.0.1",
  "info": {
    "title": "TrueWay Places API V1",
    "description": "Search for places with details information",
    "contact": {
      "name": "TrueWay Team",
      "url": "https://truewayapi.com/",
      "email": "trueway@usa.com"
    },
    "version": "v1"
  },
  "servers": [
    {
      "url": "http://api.magicapi.dev/api/v1/trueway/places"
